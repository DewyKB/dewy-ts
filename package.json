{
  "name": "dewy-ts",
  "version": "0.1.4",
  "description": "Typescript client for Dewy",
  "private": false,
  "scripts": {
    "generate-client": "rm -rf src/client && openapi --name Dewy -c fetch --input http://localhost/api/openapi.json --output ./src/client --useUnionTypes",
    "build": "tsc",
    "wtf": "openapi --help"
  },
  "dependencies": {
    "@scarf/scarf": "^1.3.0",
    "form-data": "^4",
    "langchain": "^0.1",
    "node-fetch": "^3"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.26",
    "typescript": "^4.9.5"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DewyKB/dewy-ts.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/DewyKB/dewy-ts/issues"
  },
  "homepage": "https://github.com/DewyKB/dewy-ts#readme"
}
